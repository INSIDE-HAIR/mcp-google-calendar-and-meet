{
  "name": "google-meet-mcp",
  "version": "3.0.0",
  "description": "A Model Context Protocol (MCP) server for interacting with Google Meet through the Google Calendar API",
  "main": "build/src/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "start": "tsx src/index.ts",
    "start:dev": "tsx --watch src/index.ts",
    "start:js": "node build/src/index.js",
    "setup": "tsx src/setup.ts",
    "setup:js": "node build/src/setup.js",
    "oauth-helper": "open index.html",
    "refresh-token": "./refresh-token.sh",
    "test": "vitest run",
    "test:unit": "vitest run test/*.test.ts",
    "test:integration": "vitest run test/integration.test.ts",
    "test:coverage": "vitest run --coverage",
    "test:validation": "vitest run test/validation.test.ts",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:legacy:calendars": "node test/test-calendars.ts",
    "test:legacy:basic": "node test/test-mcp.ts",
    "test:legacy:stable": "node test/test-stable-api.ts",
    "coverage": "vitest run --coverage --reporter=text-summary",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf build"
  },
  "keywords": [
    "google-meet",
    "calendar",
    "mcp",
    "model-context-protocol"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.8.0",
    "dotenv": "^17.2.1",
    "googleapis": "^126.0.0",
    "open": "^9.1.0",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/node": "^24.1.0",
    "@vitest/coverage-v8": "^3.2.4",
    "tsx": "^4.20.3",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  }
}
